{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "lib",
    "includePattern": ".js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "gh-pages/",
    "recurse": true,
    "verbose": true,
    "readme": "README.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "static": true,
    "sort": true
  },
  "markdown": {
    "highlight": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "staticFiles": {
        "include": ["docs/static"]
      }
    }
  }
}
